<?xml version="1.0" encoding="utf-8"?>
<clowdtech.mpositive.areas.till.views.SavedOrdersViewImpl xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/noOrdersIndicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="You do not currently have any saved orders"
        style="@style/text_base"/>

    <ListView
        android:id="@+id/saved_orders"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</clowdtech.mpositive.areas.till.views.SavedOrdersViewImpl>